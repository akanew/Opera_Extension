{
  "name": "Bookmarks",
  "description": "Bookmarks",
  "version": "1.0",
  "permissions": [
    "tabs",
	"http://*/*",
	"https://*/*",
	"cookies",
	"<all_urls>"
  ],
  "browser_action": {
      "default_title": "Bookmarks",
	  "default_icon": "/imgs/icon.png",
      "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
